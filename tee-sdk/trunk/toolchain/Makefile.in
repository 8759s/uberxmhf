target=@target@
INSTALL=@INSTALL@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

UTILS=strip ranlib objdump nm ld as ar cc pkg-config

all:

install:
	for UTIL in $(UTILS) ; do \
		$(INSTALL) -D $$UTIL $(bindir)/$(target)-$$UTIL ; \
	done
	ln -fs $(bindir)/$(target)-cc $(bindir)/$(target)-gcc

