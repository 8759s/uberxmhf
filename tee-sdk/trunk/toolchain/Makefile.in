PREFIX=@prefix@
ARCH=@ARCH@
OS=@OS@
INSTALL=@INSTALL@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

UTILS=strip ranlib objdump nm ld as ar cc pkg-config

install:
	for UTIL in $(UTILS) ; do \
		$(INSTALL) $$UTIL $(bindir)/$(ARCH)-$(OS)-$$UTIL ; \
	done
	ln -fs $(bindir)/$(ARCH)-$(OS)-cc $(bindir)/$(ARCH)-$(OS)-gcc

